    "--tab-radius": "0.5rem"
  },
  "mainColors": {
    "base-100": "#0B132B",
    "primary": "#FFFFFF",
    "secondary": "#c7c7c7",
    "tertiary": "#6FFFE9",
    "neutral": "#5BC0BE",
