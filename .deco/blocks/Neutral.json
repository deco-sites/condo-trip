{
  "__resolveType": "site/sections/Theme/Theme.tsx",
  "buttonStyle": {
    "--border-btn": "1px",
    "--rounded-btn": "0.2rem",
    "--btn-focus-scale": "0.95",
    "--animation-btn": "0.25s"
  },
  "colorScheme": "any",
  "otherStyles": {
    "--rounded-box": "1rem",
    "--rounded-badge": "1.9rem",
